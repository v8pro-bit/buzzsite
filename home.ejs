        <!--  <a href="https://v2.sportsonline.pro/p/wnbasports.html">WNBA</a>-->
        <!--</div>-->
      </div>
      <div class="toggle-container">
        <span class="toggle-label">Dark Mode</span>
        <label class="toggle">
          <input type="checkbox" />
          <span class="slider"></span>
        </label>
      </div>
    </div>
    <div class="container">
      <div class="left-section">
        <center>
          <script type="text/javascript">
            atOptions = {
              key: "a0859714b2709fc90fb5ab6c9089815c",
              format: "iframe",
              height: 90,
              width: 728,
              params: {},
            };
          </script>
          <script
            type="text/javascript"
            src="//starkdevicelocust.com/a0859714b2709fc90fb5ab6c9089815c/invoke.js"
          ></script>
        </center>

        <div class="matches-container">
          <% competitions.forEach((competition)=>{ %> <%
          if(competition.streams.length > 0){ %>

          <!-- League Title Section -->
          <div class="league-title">
            <img
              src="<%=BASE_URL%><%= competition.competition_logo %>"
              alt="<%= competition.competition_name %>"
            />
            <span><%= competition.competition_name %></span>
          </div>

          <% competition.streams.forEach((stream)=>{ %>

          <div class="match-box">
            <a
              href="/event/<%= stream.event_url_string %>/<%= stream.id %>"
              class="match-link-overlay"
            ></a>
            <div class="teams">
              <img
                src="<%=BASE_URL%><%= competition.competition_logo %>"
                alt="Team 1"
              />
              <span><%= stream.title %> </span>
            </div>
            <div class="match-time">
              <%= moment .tz(stream.date_time, "America/New_York")
              .format("hh:mm A") %> ET
            </div>
          </div>

          <% }) %> <% } %> <% }) %>
        </div>

        <!-- Add more matches/leagues similarly -->
      </div>

      <div class="right-section">
        <center>
          <script type="text/javascript">
            atOptions = {
              key: "f0ba85d7ac3c5abf6b70a3dc46926775",
              format: "iframe",
              height: 600,
              width: 160,
              params: {},
            };
          </script>
          <script
            type="text/javascript"
            src="//starkdevicelocust.com/f0ba85d7ac3c5abf6b70a3dc46926775/invoke.js"
          ></script>

          <script type="text/javascript">
            atOptions = {
              key: "532c2ee0d101b1f7351588131245000c",
              format: "iframe",
              height: 300,
              width: 160,
              params: {},
            };
          </script>
          <script
            type="text/javascript"
            src="//starkdevicelocust.com/532c2ee0d101b1f7351588131245000c/invoke.js"
          ></script>
        </center>
      </div>
    </div>
    <script>
      // Optional: Add script to mark active link on click
      document.addEventListener("DOMContentLoaded", () => {
        const links = document.querySelectorAll(".nav-links a");
        links.forEach((link) => {
          link.addEventListener("click", function () {
            links.forEach((l) => l.classList.remove("active"));
            this.classList.add("active");
          });
        });
      });

      const toggleCheckbox = document.querySelector(".toggle input");

      toggleCheckbox.addEventListener("change", function () {
        if (this.checked) {
          document.body.classList.add("dark-mode");
        } else {
          document.body.classList.remove("dark-mode");
        }
      });
    </script>
  </body>
</html>
