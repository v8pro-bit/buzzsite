  <body>
    <div class="header">
      <div class="header-left">
        <img
          src="https://upload.wikimedia.org/wikipedia/commons/thumb/d/d0/Den%C3%ADk_Sport_logo.svg/1200px-Den%C3%ADk_Sport_logo.svg.png"
          alt="Logo"
          class="logo"
        />
        <!--<div class="nav-links">-->
        <!--  <a href="https://v2.sportsonline.pro/">Home</a>-->
        <!--  <a href="https://v2.sportsonline.pro/p/soccerstreams.html">Soccer</a>-->
        <!--  <a href="https://v2.sportsonline.pro/p/nbasports.html">NBA</a>-->
